{
  "name": "clashbot-openapi",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "generate-rest-server": "openapi-generator-cli generate -i open-api.yml -g nodejs-express-server -o ../ClashBot-Service-OpenAPI --minimal-update -s",
    "generate-socket-server": "ag async-api.yml @asyncapi/nodejs-ws-template -o ../Clash-Bot-WS-Service -p server=localhost --force-write",
    "generate-socket-server-documentation": "ag async-api.yml @asyncapi/html-template -o output --force-write",
    "generate-angular-client": "openapi-generator-cli generate -i open-api.yml -g typescript-angular -o ../../ClashBot-AngularClient --additional-properties npmName=clash-bot-service-api,snapshot=false,ngVersion=12.2.0,npmVersion=1.0.0",
    "rest-server-dry-run-generate": "openapi-generator-cli generate -i open-api.yml -g nodejs-express-server -o ../Clash-Bot-Service-TS --minimal-update -s --dry-run"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@asyncapi/generator": "^1.9.5",
    "@asyncapi/html-template": "^0.24.10",
    "@openapitools/openapi-generator-cli": "^2.5.1"
  }
}
