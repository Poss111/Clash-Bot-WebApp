{
  "name": "clashbot-e2e",
  "version": "1.0.0",
  "description": "E2E automation for ClashBot-WebApp",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:open-reponsive": "cypress open",
    "precy": "wait-on -t 30000 http://localhost:4200 && wait-on -t 60000 http-get://localhost:8080/api/v2/health",
    "cy-integration": "cypress run --spec 'cypress/integration/e2e/*.int-spec.js",
    "cy-responsive": "cypress run --spec \"cypress/integration/responsive/*\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^9.6.0",
    "dotenv": "^10.0.0",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@applitools/eyes-cypress": "^3.27.1"
  }
}
